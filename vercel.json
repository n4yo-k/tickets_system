{
  "buildCommand": "flutter build web --release",
  "outputDirectory": "build/web",
  "env": {
    "FLUTTER_BUILD_MODE": "release"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
